{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "0.0.1",
  "icons": {
    "64": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "tsorter.js",
        "content_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icon.png"
    },
    "default_popup": "browserAction.html",
    "default_title": "__MSG_extensionName__"
  },
  "options_ui": {
    "page": "options.html"
  }
}
